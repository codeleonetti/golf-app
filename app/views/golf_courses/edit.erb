<h1> Edit Course Information </h1>
<br><br><br><br>
   
<br><br><br>
    <form method="POST" action"/golf_courses/:id/edit">

        <label> Golf Course Name: </label>
        <input type="text" name= "name"  value="<%= @course.name %>"><br/>
        <br><br>
        <label> Address: </label>
        <input type="text" name= "location" value="<%= @course.location %>"><br/>
        <br><br>
        <label> Phone Number: </label>
        <input type="text" name= "phone_number" value="<%= @course.phone_number %>"><br/>
        <br><br>
        <label> Number of Holes: </label>
        <input type="text" name= "number_of_holes" value="<%= @course.number_of_holes %>"><br/>
        <br><br>
        <label> Scorecard Info: </label>
        <input type="integer" name= "scorecard" value="<%= @course.scorecard %>"><br/>
        <br><br>
        <label> Front 9: </label>
        <input type="text" name= "front_9" value="<%= @course.front_9 %>"><br/>
        <br><br>
        <label> Back 9: </label>
        <input type="text" name= "back_9" value="<%= @course.back_9 %>"><br/>
        <br><br>
        <input type="submit" class="btn  grey darken-1">
    </form>

<br><br><br>   
    <form method="POST" action="/golfcourse/<%= @course.id %>">
        <input type="hidden" id="hidden" name="_method" value="DELETE">
        <input class="show-btns" type="submit" value="DELETE">
    </form> 
